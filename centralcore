# an object of WSGI application
from flask import Flask
app = Flask(bdx-api-link)   # Flask constructor
#bdx-api-link

# A decorator used to tells the application
# which URL is associated function
@app.route('/')
def hello():
    return 'HELLO'

if __name__=='__main__':
   app.run()
